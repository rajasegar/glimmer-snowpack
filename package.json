{
  "name": "glimmer-snowpack",
  "version": "1.0.0",
  "description": "Glimmer.js app template for Snowpack",
  "main": "index.js",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\"",
    "deploy": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "csa-template",
    "glimmer",
    "snowpack",
    "create-snowpack-app",
    "glimmerjs"
  ],
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rajasegar/glimmer-snowpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajasegar/glimmer-snowpack"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@glimmer/babel-plugin-strict-template-precompile": "^2.0.0-beta.11",
    "@snowpack/babel-plugin-package-import": "^1.1.6",
    "@snowpack/plugin-babel": "^2.1.5",
    "snowpack": "^2.18.4"
  },
  "dependencies": {
    "@glimmer/component": "2.0.0-beta.11",
    "@glimmer/core": "2.0.0-beta.11",
    "@glimmer/helper": "2.0.0-beta.11",
    "@glimmer/modifier": "2.0.0-beta.11",
    "@glimmer/tracking": "2.0.0-beta.11"
  }
